<Window x:Class="winApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:winApp"
        mc:Ignorable="d"
        Title="Test App for (gcplength-dotnet)" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="33*"/>
            <ColumnDefinition Width="17*"/>
        </Grid.ColumnDefinitions>
        <Button Content="Refresh" HorizontalAlignment="Left" Margin="20,20,0,0" VerticalAlignment="Top" Height="55" Width="100" Click="ButtonRefresh_Click"/>
        <TextBox x:Name="textboxPrefix" HorizontalAlignment="Left" Margin="27,95,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="ButtonTest" Content="Test" HorizontalAlignment="Left" Margin="27,123,0,0" VerticalAlignment="Top" Width="63" Height="23" Click="ButtonTest_Click"/>
        <Label x:Name="labelResult" Content="" HorizontalAlignment="Left" Margin="92,120,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
